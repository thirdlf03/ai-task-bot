# GitHub
GITHUB_TOKEN=ghp_xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
GITHUB_ORG=naoki
GITHUB_REPO=ai-task-bot
GITHUB_PROJECT_NUMBER=1

